{
  "compilerOptions": {
    "target": "ES2018",
    "module": "CommonJS",
    "lib": ["ES2018"],
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "skipLibCheck": true,
    "types": ["jest", "node"],
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist"]
}
